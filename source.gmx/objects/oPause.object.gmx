        instance_deactivate_object(oPlayerCopy);

    instance_activate_object(oPlayer);
}

if (global.paused)
{
    TASwatercheck();
    
    instance_deactivate_object(oPlayer);
    
    if (!global.continueclicked)
    {
        if (!instance_exists(oPlayerCopy))
            instance_create(global.pausedX,global.pausedY,oPlayerCopy);
        else 
